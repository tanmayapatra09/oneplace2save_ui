@import url("https://fonts.googleapis.com/css2?family=Roboto:wght@100;300;400;500;700&display=swap");
@import url("https://fonts.googleapis.com/css2?family=Bebas+Neue&display=swap");
* {
	padding: 0px;
	margin: 0px;
}

body {
	background: #fffbfb;
	height: 100vh;
	font-family: "Roboto", sans-serif !important;
}

*.hidden {
	display: none !important;
}

div.loading {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background-color: rgba(16, 16, 16, 0.5);
}

@-webkit-keyframes uil-ring-anim {
	0% {
		-ms-transform: rotate(0deg);
		-moz-transform: rotate(0deg);
		-webkit-transform: rotate(0deg);
		-o-transform: rotate(0deg);
		transform: rotate(0deg);
	}
	100% {
		-ms-transform: rotate(360deg);
		-moz-transform: rotate(360deg);
		-webkit-transform: rotate(360deg);
		-o-transform: rotate(360deg);
		transform: rotate(360deg);
	}
}

@-webkit-keyframes uil-ring-anim {
	0% {
		-ms-transform: rotate(0deg);
		-moz-transform: rotate(0deg);
		-webkit-transform: rotate(0deg);
		-o-transform: rotate(0deg);
		transform: rotate(0deg);
	}
	100% {
		-ms-transform: rotate(360deg);
		-moz-transform: rotate(360deg);
		-webkit-transform: rotate(360deg);
		-o-transform: rotate(360deg);
		transform: rotate(360deg);
	}
}

@-moz-keyframes uil-ring-anim {
	0% {
		-ms-transform: rotate(0deg);
		-moz-transform: rotate(0deg);
		-webkit-transform: rotate(0deg);
		-o-transform: rotate(0deg);
		transform: rotate(0deg);
	}
	100% {
		-ms-transform: rotate(360deg);
		-moz-transform: rotate(360deg);
		-webkit-transform: rotate(360deg);
		-o-transform: rotate(360deg);
		transform: rotate(360deg);
	}
}

@-ms-keyframes uil-ring-anim {
	0% {
		-ms-transform: rotate(0deg);
		-moz-transform: rotate(0deg);
		-webkit-transform: rotate(0deg);
		-o-transform: rotate(0deg);
		transform: rotate(0deg);
	}
	100% {
		-ms-transform: rotate(360deg);
		-moz-transform: rotate(360deg);
		-webkit-transform: rotate(360deg);
		-o-transform: rotate(360deg);
		transform: rotate(360deg);
	}
}

@-moz-keyframes uil-ring-anim {
	0% {
		-ms-transform: rotate(0deg);
		-moz-transform: rotate(0deg);
		-webkit-transform: rotate(0deg);
		-o-transform: rotate(0deg);
		transform: rotate(0deg);
	}
	100% {
		-ms-transform: rotate(360deg);
		-moz-transform: rotate(360deg);
		-webkit-transform: rotate(360deg);
		-o-transform: rotate(360deg);
		transform: rotate(360deg);
	}
}

@-webkit-keyframes uil-ring-anim {
	0% {
		-ms-transform: rotate(0deg);
		-moz-transform: rotate(0deg);
		-webkit-transform: rotate(0deg);
		-o-transform: rotate(0deg);
		transform: rotate(0deg);
	}
	100% {
		-ms-transform: rotate(360deg);
		-moz-transform: rotate(360deg);
		-webkit-transform: rotate(360deg);
		-o-transform: rotate(360deg);
		transform: rotate(360deg);
	}
}

@-o-keyframes uil-ring-anim {
	0% {
		-ms-transform: rotate(0deg);
		-moz-transform: rotate(0deg);
		-webkit-transform: rotate(0deg);
		-o-transform: rotate(0deg);
		transform: rotate(0deg);
	}
	100% {
		-ms-transform: rotate(360deg);
		-moz-transform: rotate(360deg);
		-webkit-transform: rotate(360deg);
		-o-transform: rotate(360deg);
		transform: rotate(360deg);
	}
}

@keyframes uil-ring-anim {
	0% {
		-ms-transform: rotate(0deg);
		-moz-transform: rotate(0deg);
		-webkit-transform: rotate(0deg);
		-o-transform: rotate(0deg);
		transform: rotate(0deg);
	}
	100% {
		-ms-transform: rotate(360deg);
		-moz-transform: rotate(360deg);
		-webkit-transform: rotate(360deg);
		-o-transform: rotate(360deg);
		transform: rotate(360deg);
	}
}

.uil-ring-css {
	margin: auto;
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	width: 200px;
	height: 200px;
}

.uil-ring-css>div {
	position: absolute;
	display: block;
	width: 160px;
	height: 160px;
	top: 20px;
	left: 20px;
	border-radius: 80px;
	box-shadow: 0 6px 0 0 #ffffff;
	-ms-animation: uil-ring-anim 1s linear infinite;
	-moz-animation: uil-ring-anim 1s linear infinite;
	-webkit-animation: uil-ring-anim 1s linear infinite;
	-o-animation: uil-ring-anim 1s linear infinite;
	animation: uil-ring-anim 1s linear infinite;
}

.container-fluid {
	width: 80%;
}

.header {
	margin: 0px auto;
	float: left;
	width: 100%;
	background: linear-gradient(#ffeb00, #ffd000);
	-webkit-box-shadow: 0 2px 28px 0 rgb(0 0 0 / 9%);
	box-shadow: 0 2px 28px 0 rgb(0 0 0 / 9%);
	position: fixed;
	z-index: 10;
}

.logo-section {
	width: 20%;
	float: left;
}

.logo-section img {
	max-width: 100%;
}

.navbar {
	padding: 0px;
	float: left;
	margin-left: 5%;
}

.navbar .navbar-collapse {
	height: 100%;
}

.navbar .navbar-collapse .navbar-nav {
	height: 100%;
}

header .nav-item {
	padding: 12px 4px;
	font-weight: 500;
	transition: 0.6s all;
}

header .nav-item:hover,
header .nav-item:focus {
	background: #ff0000;
	color: #fff;
	transition: 0.6s all;
}

header .nav-item:hover .nav-link,
header .nav-item:focus .nav-link {
	color: #fff !important;
}

header .nav-item {
	position: relative;
}

header .nav-item .nav-link {
	color: #000 !important;
	padding-top: 14px;
	font-weight: 500;
	font-size: 14px;
}

header .nav-item.active {
	background: #ff1e1e;
}

.dropdown {
	transition: ease-out 0.35s;
	-moz-transition: ease-out 0.35s;
	-webkit-transition: ease-out 0.35s;
	list-style-type: none;
}


/*First Level*/

.subs {
	left: 0;
	position: relative;
	top: 25px;
	width: 175px;
	border-left: none !important;
	border-bottom: 1px solid #fff !important;
	background-color: #ff1e1e;
	font-size: 14px;
}

.subs a {
	padding: 8px 12px;
	display: block;
}

.subs:last-child {
	border: none !important;
}

.hassubs:hover .dropdown,
.hassubs .hassubs:hover .dropdown {
	opacity: 1;
	visibility: visible;
	transition: ease-in 0.35s;
	-moz-transition: ease-in 0.35s;
	-webkit-transition: ease-in 0.35s;
}

.navbar-nav li:hover ul a,
.navbar-nav li:hover ul li ul li a {
	color: #fff;
}

.navbar-nav li ul li:hover,
.navbar-nav li ul li ul li:hover {
	background: #fff;
	transition: ease-in-out 0.35s;
	-moz-transition: ease-in-out 0.35s;
	-webkit-transition: ease-in-out 0.35s;
}

.navbar-nav li ul li:hover a,
.navbar-nav li ul li ul li:hover a {
	color: #ffd100;
	transition: ease-in-out 0.35s;
	-moz-transition: ease-in-out 0.35s;
	-webkit-transition: ease-in-out 0.35s;
	text-decoration: none;
}


/*Second Level*/

.hassubs .hassubs .dropdown .subs {
	left: 54px;
	position: relative;
	width: 160px;
	top: 0px;
}

.hassubs .hassubs .dropdown {
	position: absolute;
	width: 150px;
	left: 120px;
	top: 0px;
	opacity: 0;
	visibility: hidden;
	transition: ease-out 0.35s;
	-moz-transition: ease-out 0.35s;
	-webkit-transition: ease-out 0.35s;
}

