p.title {
	font-size: 28px;
	font-weight: 500;
	position: relative;
	text-transform: uppercase;
	text-align: center;
	margin-bottom: 3rem;
}

p.title::before {
	content: "";
	position: absolute;
	bottom: -15px;
	width: 230px;
	height: 1px;
	right: 0;
	left: 0;
	margin: 0 auto;
	background: rgb(77, 77, 77);
}

p.title::after {
	content: "";
	position: absolute;
	bottom: -16px;
	left: 0;
	right: 0;
	margin: 0 auto;
	height: 4px;
	width: 100px;
	border-radius: 5px;
	background: linear-gradient(45deg, #ffd100, #ffe800);
}

