/* Menu Styles */
.logo {
  padding: 12px 0;
	text-align: center;
}
.logo img {
  max-width: 100%;
}
.number_box span:last-child a {
  font-size: 18px;
}
.number_box span:first-child {
  font-size: 11px;
}
#menu ul {
  margin: 0;
  padding: 0;
}

#menu .main-menu {
  display: none;
}

#tm:checked + .main-menu {
  display: block;
}

#menu input[type="checkbox"],
#menu ul span.drop-icon {
  display: none;
}

#menu li,
#toggle-menu,
#menu .sub-menu {
  border-style: solid;
  border-color: rgba(0, 0, 0, .05);
}
#menu li:last-of-type {
	border: 0;
}
#menu li,
#toggle-menu {
  border-width: 0 0 1px;
}

#menu .sub-menu {
  background-color: #ffd000;
  border-width: 1px 1px 0;
  margin: 0 1em;
}

#menu .sub-menu li:last-child {
  border-width: 0;
}

#menu li,
#toggle-menu,
#menu a {
  position: relative;
  display: block;
  color: #222;
}

#toggle-menu {
  background-color: #ff0000;
}
#toggle-menu {
	color: #fff;
	padding: .8em 1em;
	font-size: 14px;
	font-weight: 500;
	cursor: pointer;
}
#menu a {
  padding: 1.6em .8em;
	font-size: 14px;
	font-weight: 500;
}

#menu a {
  transition: all .125s ease-in-out;
  -webkit-transition: all .125s ease-in-out;
}

#menu a:hover, #menu li.active > a {
  background-color: #ff0c0c;
  color: #fff;
	text-decoration: none;
}

#menu .sub-menu {
  display: none;
}
#menu .sub-menu li a  {
	padding: .8em 1em;
	font-weight: 400;
}
#menu input[type="checkbox"]:checked + .sub-menu {
  display: block;
}

#menu .sub-menu a:hover {
  color: #fff;
}

#toggle-menu .drop-icon,
#menu li label.drop-icon {
  position: absolute;
  right: 1em;
  top: .8em;
	cursor: pointer;
}

#menu label.drop-icon, #toggle-menu span.drop-icon {
  border-radius: 50% 50%;
  width: 1.5em;
  height: 1.5em;
  text-align: center;
  background-color: rgba(0, 0, 0, .125);
  text-shadow: 0 0 0 transparent;
  color: rgba(255, 255, 255, .75);
}

#menu .drop-icon {
  line-height: 1.5;
}
@media only screen and (max-width: 64em) and (min-width: 52.01em) {
  #menu li {
    width: 33.333%;
  }

  #menu .sub-menu li {
    width: auto;
  }
}

@media only screen and (min-width: 52em) {

  #menu .main-menu {
    display: table;
		margin: 0 auto;
  }

  #toggle-menu,
  #menu label.drop-icon {
    display: none;
  }

  #menu ul span.drop-icon {
    display: inline-block;
  }

  #menu li {
    float: left;
    border-width: 0 1px 0 0;
  }

  #menu .sub-menu li {
    float: none;
  }

  #menu .sub-menu {
    border-width: 0;
    margin: 0;
    position: absolute;
    top: 100%;
    left: 0;
    width: 12em;
    z-index: 3000;
  }

  #menu .sub-menu,
  #menu input[type="checkbox"]:checked + .sub-menu {
    display: none;
  }

  #menu .sub-menu li {
    border-width: 0 0 1px;
  }

  #menu .sub-menu .sub-menu {
    top: 0;
    left: 100%;
  }

  #menu li:hover > input[type="checkbox"] + .sub-menu {
    display: block;
  }
}
@media only screen and (max-width: 640px) {
	#menu a {
		padding: .8em 1.5em;
	}
	#menu .sub-menu {
		background-color: #fff;
